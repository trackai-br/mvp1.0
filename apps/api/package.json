    "@aws-sdk/client-secrets-manager": "^3.995.0",
    "@aws-sdk/client-sqs": "^3.995.0",
    "@fastify/cors": "^11.2.0",
    "@fastify/env": "^5.0.3",
    "@fastify/sensible": "^6.0.4",
    "@hub/shared": "0.1.0",
    "@prisma/adapter-pg": "^7.4.1",
    "@prisma/client": "^7.4.1",
    "axios": "^1.13.5",
    "bullmq": "^5.69.3",
    "dotenv": "^17.3.1",
    "fastify": "^5.7.4",
    "fastify-type-provider-zod": "^6.1.0",
    "ioredis": "^5.9.3",
    "pg": "^8.13.0",
    "pino": "^10.3.1",
    "pino-pretty": "^13.1.3",
    "prisma": "^7.4.1",
    "undici": "^7.22.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.3.0",
    "@types/pg": "^8.11.0",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "eslint": "^9.39.2",
    "globals": "^16.4.0",
    "supertest": "^7.2.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
